{
  "name": "wedo/api",
  "description": "rest api",
  "license": ["MIT"],
  "require": {
    "php": "^8.1",
    "psr/log": "^1.1",
    "nette/di": "^3.0",
    "nette/application": "^3.0",
    "nette/forms": "^3.1.2",
    "wedo/utilities": "^2.0.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-dibi": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "contributte/qa": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "Wedo\\Api\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wedo\\Api\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
